{
  "name": "npm-click",
  "version": "0.0.1",
  "description": "Comparing NPM (dev)Dependencies.",
  "main": "src/js/app.js",
  "scripts": {
    "mkdirs": "mkdir -p dist/js && mkdir -p dist/fonts",
    "copy-fonts": "cp -r node_modules/font-awesome/fonts/* dist/fonts/",
    "build-js": "browserify -d -t babelify src/js/index.js -t envify -p [tsify -p tsconfig.json] -p [minifyify --map bundle.map.js --output dist/js/bundle.map.js] -o dist/js/index.js",
    "build-scss": "node-sass --output-style compressed src/scss/app.scss dist/css/app.css",
    "build": "npm run mkdirs && npm run copy-fonts && npm run build-js && npm run build-scss",
    "watch-js": "watchify -d -t babelify src/js/index.js -t envify -p [tsify -p tsconfig.json] -o dist/js/index.js -v",
    "watch-scss": "node-sass -w src/scss/app.scss dist/css/app.css",
    "watch": "npm run mkdirs && npm run watch-js && npm run watch-scss",
    "prettier": "prettier --single-quote --trailing-comma es5 --write 'src/js/**/*.js'",
    "prettier-check": "./scripts/prettier-check",
    "lint": "tslint --project tsconfig.json",
    "test": "npm run prettier-check && npm run lint",
    "start": "http-server './' -c-1 -o"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekonstantinidis/npm-click.git"
  },
  "keywords": [
    "npm",
    "dependencies",
    "version",
    "semver",
    "compare",
    "github",
    "package.json"
  ],
  "author": "Emmanouil Konstantinidis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ekonstantinidis/npm-click/issues"
  },
  "homepage": "http://www.npm.click/",
  "dependencies": {
    "@types/chart.js": "2.7.3",
    "@types/react": "=16.0.31",
    "@types/react-dom": "=16.0.3",
    "@types/react-dropzone": "=4.1.0",
    "babel-preset-es2015": "=6.24.1",
    "babel-preset-react": "=6.24.1",
    "babel-preset-stage-0": "=6.24.1",
    "babelify": "=7.3.0",
    "bootstrap": "=4.0.0-alpha.2",
    "browserify": "=13.0.1",
    "chart.js": "2.7.1",
    "envify": "=4.1.0",
    "font-awesome": "=4.6.3",
    "immutable": "=3.8.2",
    "malarkey": "=1.3.3",
    "minifyify": "=7.3.5",
    "prop-types": "15.6.0",
    "react": "=16.2.0",
    "react-chartjs-2": "2.6.4",
    "react-dom": "=16.2.0",
    "react-dropzone": "=4.2.3",
    "react-redux": "=5.0.6",
    "react-router-dom": "4.2.2",
    "react-router-redux": "=4.0.5",
    "redux": "=3.7.2",
    "redux-api-middleware": "=1.0.2",
    "redux-thunk": "=2.2.0",
    "tsify": "=3.0.4",
    "typescript": "2.6.2",
    "underscore": "=1.8.3"
  },
  "devDependencies": {
    "lodash.assign": "=4.0.9",
    "node-sass": "=4.7.2",
    "prettier": "=1.9.2",
    "tslint": "5.8.0",
    "tslint-config-dabapps": "dabapps/tslint-config-dabapps#v0.4.0",
    "tslint-config-prettier": "1.6.0",
    "watchify": "=3.9.0"
  }
}
