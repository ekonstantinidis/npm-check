{
  "name": "npm-click",
  "version": "0.0.1",
  "description": "Comparing NPM (dev)Dependencies.",
  "main": "src/js/app.js",
  "scripts": {
    "copy-fonts": "mkdir -p dist/fonts && cp -r node_modules/font-awesome/fonts/* dist/fonts/",
    "build-js": "mkdir -p dist/js && browserify -v -d -t babelify src/js/app.js -t envify -p [tsify] -p [minifyify --map app.map.js --output dist/js/app.map.js] -o dist/js/app.js",
    "build-scss": "mkdir -p dist/css && node-sass --output-style compressed src/scss/app.scss dist/css/app.css",
    "build": "npm run copy-fonts && npm run build-js && npm run build-scss",
    "watch-js": "watchify -t babelify -p [tsify -p tsconfig.json] src/js/app.js -o dist/js/app.js -v",
    "watch-scss": "node-sass -w src/scss/app.scss dist/css/app.css",
    "watch": "npm run build && npm run watch-js & npm run watch-scss",
    "prettier": "prettier --single-quote --trailing-comma es5 --write 'src/js/**/*.js'",
    "prettier-check": "./scripts/prettier-check",
    "test": "eslint 'src/js/'",
    "start": "http-server './' -c-1 -o",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manosim/npm-click.git"
  },
  "keywords": [
    "npm",
    "dependencies",
    "version",
    "semver",
    "compare",
    "github",
    "package.json"
  ],
  "author": "Emmanouil Konstantinidis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manosim/npm-click/issues"
  },
  "homepage": "https://www.npm.click/",
  "dependencies": {
    "@types/chart.js": "=2.7.3",
    "@types/nprogress": "=0.0.29",
    "@types/react": "=16.0.31",
    "@types/react-dom": "=16.0.3",
    "@types/react-dropzone": "4.1.0",
    "@types/react-redux": "5.0.14",
    "axios": "=0.17.1",
    "babel-core": "=6.26.0",
    "babelify": "=7.3.0",
    "bootstrap": "=4.0.0-alpha.2",
    "browserify": "=14.5.0",
    "chart.js": "=2.7.1",
    "envify": "=4.1.0",
    "font-awesome": "=4.7.0",
    "immutable": "=3.8.2",
    "malarkey": "=1.3.3",
    "minifyify": "=7.3.5",
    "node-sass": "=4.7.2",
    "nprogress": "=0.2.0",
    "prop-types": "15.6.0",
    "react": "=16.2.0",
    "react-chartjs-2": "2.6.4",
    "react-dom": "=16.2.0",
    "react-dropzone": "=4.2.3",
    "react-redux": "=5.0.6",
    "redux": "=3.7.2",
    "redux-api-middleware": "=1.0.2",
    "redux-thunk": "=2.2.0",
    "semver": "5.4.1",
    "tsify": "=3.0.4",
    "typescript": "=2.6.2",
    "underscore": "=1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "=8.0.3",
    "babel-preset-es2015": "=6.24.1",
    "babel-preset-react": "=6.24.1",
    "babel-preset-stage-0": "=6.24.1",
    "eslint": "=4.13.1",
    "eslint-plugin-react": "=7.5.1",
    "lodash.assign": "=4.2.0",
    "prettier": "=1.9.2",
    "watchify": "=3.9.0"
  }
}
