{
  "name": "npm-click",
  "version": "0.0.1",
  "description": "Comparing NPM (dev)Dependencies.",
  "main": "src/js/app.js",
  "scripts": {
    "build-js": "webpack",
    "build": "npm install && mkdir -p build/js && grunt build && npm run build-js",
    "watch-js": "webpack --progress --colors --watch",
    "watch": "grunt build && npm build && npm run watch-js | grunt watch",
    "release-js": "webpack --config webpack.prod.config.js",
    "release": "npm install && mkdir -p build/js && grunt build && npm run release-js",
    "test": "eslint 'src/js/'",
    "start": "http-server './build/' -c-1 -o"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ekonstantinidis/npm-click.git"
  },
  "keywords": [
    "npm",
    "dependencies",
    "version",
    "semver",
    "compare",
    "github",
    "package.json"
  ],
  "author": "Emmanouil Konstantinidis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ekonstantinidis/npm-click/issues"
  },
  "homepage": "http://www.npm.click/",
  "dependencies": {
    "bootstrap": "=4.0.0-alpha.2",
    "chart.js": "=2.1.5",
    "font-awesome": "=4.6.3",
    "history": "=3.0.0",
    "react": "=15.1.0",
    "react-chartjs": "=0.7.3",
    "react-dom": "=15.1.0",
    "react-dropzone": "=3.5.1",
    "react-router": "=2.4.1",
    "reloading": "0.0.6"
  },
  "devDependencies": {
    "babel-core": "=6.9.1",
    "babel-eslint": "=6.0.4",
    "babel-loader": "=6.2.4",
    "babel-preset-es2015": "=6.9.0",
    "babel-preset-react": "=6.5.0",
    "babel-preset-stage-0": "=6.5.0",
    "babelify": "=7.3.0",
    "browserify": "=13.0.1",
    "eslint": "=2.12.0",
    "eslint-plugin-react": "=5.1.1",
    "gulp": "=3.9.1",
    "gulp-sass": "=2.3.1",
    "gulp-uglify": "=1.5.3",
    "gulp-util": "=3.0.7"
  }
}
