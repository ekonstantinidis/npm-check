{
  "name": "npm-click",
  "version": "0.0.1",
  "description": "Comparing NPM (dev)Dependencies.",
  "main": "src/js/app.js",
  "scripts": {
    "build-js": "mkdir -p dist/js && browserify -t babelify src/js/app.js -t [ envify --NODE_ENV production ] -t uglifyify -p bundle-collapser/plugin | uglifyjs -c -m --source-map -o dist/js/app.js",
    "build-scss": "mkdir -p dist/css && node-sass --output-style compressed src/scss/app.scss dist/css/app.css",
    "build": "npm run build-js && npm run build-scss",
    "watch-js": "watchify -t babelify src/js/app.js -o dist/js/app.js -v",
    "watch-scss": "node-sass -w src/scss/app.scss dist/css/app.css",
    "watch": "npm run build && npm run watch-js & npm run watch-scss",
    "test": "eslint 'src/js/'",
    "start": "http-server './' -c-1 -o",
    "postinstall": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/manosim/npm-click.git"
  },
  "keywords": [
    "npm",
    "dependencies",
    "version",
    "semver",
    "compare",
    "github",
    "package.json"
  ],
  "author": "Emmanouil Konstantinidis",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manosim/npm-click/issues"
  },
  "homepage": "https://www.npm.click/",
  "dependencies": {
    "add": "2.0.6",
    "babel-core": "=6.26.0",
    "babelify": "=7.3.0",
    "bootstrap": "=4.0.0-alpha.2",
    "browserify": "=14.5.0",
    "bundle-collapser": "=1.3.0",
    "chart.js": "=2.7.1",
    "envify": "=4.1.0",
    "font-awesome": "=4.7.0",
    "immutable": "=3.8.2",
    "malarkey": "=1.3.3",
    "node-sass": "4.7.2",
    "prop-types": "15.6.0",
    "react": "=16.2.0",
    "react-chartjs-2": "2.6.4",
    "react-dom": "=16.2.0",
    "react-dropzone": "=4.2.3",
    "react-redux": "=5.0.6",
    "react-router-dom": "=4.2.0",
    "redux": "=3.7.2",
    "redux-api-middleware": "=1.0.2",
    "redux-thunk": "=2.2.0",
    "uglify-js": "=3.2.2",
    "uglifyify": "=4.0.5",
    "underscore": "=1.8.3"
  },
  "devDependencies": {
    "babel-eslint": "=8.0.3",
    "babel-preset-es2015": "=6.24.1",
    "babel-preset-react": "=6.24.1",
    "babel-preset-stage-0": "=6.24.1",
    "eslint": "=4.13.1",
    "eslint-plugin-react": "=7.5.1",
    "lodash.assign": "=4.2.0",
    "prettier": "1.9.2",
    "redux-logger": "=3.0.6",
    "watchify": "=3.9.0"
  }
}
